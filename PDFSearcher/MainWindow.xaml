<Window x:Class="PDFSearcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PDFSearcher"
        mc:Ignorable="d"
        ShowInTaskbar="False"
        Visibility="Visible"
        Title="MainWindow" Height="100" Width="100">
    <Window.Resources >
        <ContextMenu x:Key="NotifyIconContextMenu" Placement="MousePoint">
            <MenuItem Header="Close" Click="Menu_Close"  />
            <MenuItem Header="Library search" Click="Library_Search" />
        </ContextMenu>
    </Window.Resources>
    <Grid>
    </Grid>
</Window>
